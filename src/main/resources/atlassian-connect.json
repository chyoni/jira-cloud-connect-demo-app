  "baseUrl": "${addon.base-url}",
  "name": "Demo App",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": ["READ", "WRITE"],

  "modules": {
